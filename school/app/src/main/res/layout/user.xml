<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/login_text_change_pwd10"
        android:layout_width="122dp"
        android:layout_height="20dp"
        android:layout_below="@+id/book"
        android:layout_centerHorizontal="true"
        android:text="个人中心"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/login_text_change_pwd9"
        android:layout_width="122dp"
        android:layout_height="20dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/book"
        android:text="联系我们"
        android:textSize="15dp"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/login_text_change_pwd2"
        android:layout_width="122dp"
        android:layout_height="20dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/tiyu"
        android:text="二手生活用品"
        android:textSize="15dp"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/login_text_change_pwd3"
        android:layout_width="122dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/book"
        android:text="二手图书"
        android:textSize="15dp"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/login_text_change_pwd8"
        android:layout_width="122dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bike"
        android:layout_marginStart="0dp"
        android:text="二手自行车"
        android:textSize="15dp"
        android:layout_alignParentLeft="true"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/contect"
        android:layout_width="126dp"
        android:layout_height="98dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/book"
        android:background="#ffffff"
        android:layout_alignParentRight="true" />

    <ImageView
        android:id="@+id/person"
        android:layout_width="126dp"
        android:layout_height="99dp"
        android:layout_alignEnd="@+id/tiyu"
        android:layout_alignTop="@+id/book"
        android:background="#ffffff"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/computer"
        android:layout_width="126dp"
        android:layout_height="98dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/electric"
        android:background="#ffffff"
        android:layout_alignParentRight="true" />

    <TextView
        android:id="@+id/login_text_change_pwd5"
        android:layout_width="122dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/electric"
        android:layout_marginTop="-171dp"
        android:text="二手电子产品"
        android:textSize="15dp"
        android:layout_alignParentLeft="true" />

    <ImageView
        android:id="@+id/work"
        android:layout_width="126dp"
        android:layout_height="98dp"


        android:layout_alignTop="@+id/electric"
        android:layout_centerHorizontal="true"
        android:background="#ffffff" />

    <TextView
        android:id="@+id/login_text_change_pwd6"
        android:layout_width="122dp"
        android:layout_height="20dp"
        android:layout_below="@+id/electric"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-171dp"
        android:text="二手办公用品"
        android:textSize="15dp" />

    <TextView
        android:id="@+id/login_text_change_pwd7"
        android:layout_width="130dp"
        android:layout_height="20dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/electric"
        android:layout_marginTop="-171dp"
        android:text="二手电脑配件"
        android:textSize="15dp"
        android:layout_alignParentRight="true" />

    <ImageView
        android:id="@+id/book"
        android:layout_width="126dp"
        android:layout_height="101dp"


        android:layout_alignParentStart="true"
        android:layout_below="@+id/login_text_change_pwd5"
        android:background="#ffffff"
        android:layout_alignParentLeft="true" />

    <ImageView
        android:id="@+id/electric"
        android:layout_width="126dp"
        android:layout_height="98dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="171dp"
        android:background="#ffffff"
        android:layout_alignParentLeft="true" />

    <ImageView
        android:id="@+id/bike"
        android:layout_width="128dp"
        android:layout_height="87dp"
        android:layout_below="@+id/logo"
        android:background="#ffffff" />

    <ImageView
        android:id="@+id/life"
        android:layout_width="128dp"
        android:layout_height="88dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/logo"
        android:background="#ffffff"
        android:layout_alignParentRight="true" />

    <TextView
        android:id="@+id/login_text_change_pwd4"
        android:layout_width="122dp"
        android:layout_height="20dp"
        android:layout_below="@+id/tiyu"
        android:layout_centerHorizontal="true"
        android:text="二手体育用品"
        android:textSize="15dp" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="300dp"
        android:layout_height="126dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignWithParentIfMissing="false"
        android:background="#ffffff" />


    <ImageView
        android:id="@+id/tiyu"
        android:layout_width="126dp"
        android:layout_height="88dp"
        android:layout_below="@+id/logo"
        android:layout_centerHorizontal="true"
        android:background="#ffffff" />

    <Button
        android:id="@+id/returnback"
        android:layout_width="match_parent"
        android:layout_height="31dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="#d95b5b"
        android:onClick="back_to_login"
        android:text="退出"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_alignParentLeft="true" />



</RelativeLayout>
